{
  "name": "@gorules/jdm-editor",
  "version": "0.0.1",
  "description": "",
  "author": "GoRules <hi@gorules.io> (https://gorules.io)",
  "homepage": "https://github.com/gorules/zen",
  "license": "MIT",
  "keywords": [],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE.md"
  ],
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "build": "run-s build:del build:rollup",
    "build:rollup": "rollup -c",
    "build:del": "del dist",
    "dev": "rollup -c -w",
    "storybook": "storybook dev -p 9009",
    "build-storybook": "storybook build -o docs",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --check 'src/**/*.{ts?(x),css}'",
    "prettier:fix": "yarn prettier --write",
    "format": "run-s lint prettier",
    "format:fix": "run-s lint:fix prettier:fix",
    "test": "react-scripts test",
    "test:coverage": "yarn test --coverage --watchAll"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@ant-design/cssinjs": "1.0.2",
    "enhanced-resolve": "5.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.5",
    "@rollup/plugin-commonjs": "23.0.3",
    "@rollup/plugin-node-resolve": "15.0.1",
